<project name="Hipparchus">
  <skin>
    <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.1.1</version>
  </skin>
  <bannerLeft>
    <name><![CDATA[<img src="images/logo-hipparchus-blue.png" style="width:209px;height:136px;"/>]]></name>
    <href>index.html</href>
  </bannerLeft>
  <custom>
    <reflowSkin>
      <theme>bootswatch-readable</theme>
      <localResources>true</localResources>
      <highlightJs>true</highlightJs>
      <titleTemplate>%2$s | %1$s</titleTemplate>
      <toc>top</toc>
      <topNav>Hipparchus|User Guide|Parent|Modules|Reports</topNav>
      <bottomNav>
        <column>License</column>
        <column>GitHub</column>
        <column>Migrating from Apache Commons Math</column>
      </bottomNav>
      <bottomDescription>
        Hipparchus is a pure Java library for mathematics and numerical analysis.
      </bottomDescription>
      <endContent>
        <script type="text/javascript" src="https://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-AMS-MML_HTMLorMML"></script>
      </endContent>
      <pages>
        <index project="hipparchus-aggregator">
          <shortTitle>About</shortTitle>
          <breadcrumbs>false</breadcrumbs>
          <toc>false</toc>
          <sections>
            <columns>2</columns>
            <body/>
            <sidebar/>
          </sections>
        </index>
        <building>
          <highlightJs>false</highlightJs>
          <body/>
        </building>
        <license>
          <highlightJs>false</highlightJs>
          <body/>
        </license>
        <rat-report>
          <highlightJs>false</highlightJs>
          <body/>
        </rat-report>
      </pages>
    </reflowSkin>
  </custom>

  <body>
    <head>
      <link rel="stylesheet" href="https://yandex.st/highlightjs/7.5/styles/default.min.css"/>
      <script src="https://yandex.st/highlightjs/7.5/highlight.min.js"></script>
    </head>

    <menu name="Hipparchus">
      <item name="Overview"                      href="index.html"/>
      <item name="Building"                      href="building.html"/>
      <item name="Downloads"                     href="downloads.html"/>
      <item name="Mailing lists"                 href="mail-lists.html"/>
      <item name="Latest API docs (development)" href="apidocs/index.html"/>
      <item name="1.0 API docs"                  href="https://hipparchus.org/apidocs-1.0/index.html"/>
      <item name="1.1 API docs"                  href="https://hipparchus.org/apidocs-1.1/index.html"/>
      <item name="1.2 API docs"                  href="https://hipparchus.org/apidocs-1.2/index.html"/>
      <item name="Source Repository (current)"   href="https://github.com/Hipparchus-Math/hipparchus"/>
      <item name="Developers Guide"              href="developers.html"/>
      <item name="User Guide"                    href="user-guide.html" />
      <item name="Release Guide"                 href="release-guide.html" />
    </menu>

    <menu name="Modules" ref="modules"/>

    <menu name="Reports" ref="reports"/>

    <menu name ="License">
      <item name="License" href="license.html"/>
    </menu>

    <menu name ="GitHub">
      <item name="GitHub repository" href="https://github.com/Hipparchus-Math/hipparchus"/>
    </menu>

    <menu name ="Migrating from Apache Commons Math">
      <item name="Migration module"     href="hipparchus-migration/index.html"/>
      <item name="Migration script help" href="hipparchus-migration/script.html"/>
